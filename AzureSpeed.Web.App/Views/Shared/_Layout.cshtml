<!DOCTYPE html>
<html ng-app="azurespeed" ng-controller="MainController">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="keywords" content="Azure, Microsoft Azure, Azure Storage, Azure network latency, Azure Blob Upload Speed" />
    <meta name="description" content="AzureSpeed.com - Test network latencies and speed to Azure data centers from different countries." />
    <title>@ViewBag.Title - Azure Speed Test</title>

    <environment names="Development">
        <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
        <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
        <link href="~/lib/metisMenu/metisMenu.css" rel="stylesheet" />
        <link href="~/css/sb-admin-2.css" rel="stylesheet" />
        <link href="~/css/site.css" rel="stylesheet" />
    </environment>
    <environment names="Staging,Production">
        <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
        <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/lib/metisMenu/metisMenu.min.css" rel="stylesheet" />
        <link href="~/css/site.min.css" rel="stylesheet" />
    </environment>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.4.min.js"></script>
    @Html.Partial("_Analytics")
</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="~/">AzureSpeed.com</a>
            </div>
            <!-- /.navbar-header -->
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a href="~/Azure/Latency"><i class="fa fa-send fa-fw"></i> Latency Test</a>
                        </li>
                        <li>
                            <a href="~/Azure/CDN"><i class="fa fa-cloud fa-fw"></i> CDN Test</a>
                        </li>
                        <li>
                            <a href="~/Azure/Upload"><i class="fa fa-cloud-upload fa-fw"></i> Upload Speed Test</a>
                        </li>
                        <li>
                            <a href="~/Azure/UploadLargeFile"><i class="fa fa-upload fa-fw"></i> Large File Upload Speed Test</a>
                        </li>
                        <li>
                            <a href="~/Azure/Download"><i class="fa fa-cloud-download fa-fw"></i> Download Speed Test</a>
                        </li>
                        <li>
                            <a href="~/Cloud/IpRange"><i class="fa fa-list-alt fa-fw"></i> Cloud IP Range</a>
                        </li>
                        <li>
                            <a href="~/Cloud/RegionFinder"><i class="fa fa-search fa-fw"></i> Cloud Region Finder</a>
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-files-o fa-fw"></i> Azure Online Tools<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="~/AzureOnlineTools/EnableStorageCORS">Enable Storage CORS</a>
                                </li>
                                <li>
                                    <a href="~/AzureOnlineTools/GenerateEventHubSAS">EventHub SAS Generator</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="~/Azure/Reference"><i class="fa fa-book fa-fw"></i> Reference</a>
                        </li>
                        <li>
                            <a href="~/Azure/About"><i class="fa fa-book fa-fw"></i> About</a>
                        </li>
                    </ul>
                    <!-- /#side-menu -->
                    <div class="container">
                        <a href="https://twitter.com/share" class="twitter-share-button" data-url="http://www.azurespeed.com" data-hashtags="azurespeed">Tweet</a>
                        <script>
                            !function (d, s, id) {
                                var js, fjs = d.getElementsByTagName(s)[0], p = /^http:/.test(d.location) ? 'http' : 'https';
                                if (!d.getElementById(id)) {
                                    js = d.createElement(s);
                                    js.id = id;
                                    js.src = p + '://platform.twitter.com/widgets.js';
                                    fjs.parentNode.insertBefore(js, fjs);
                                }
                            }(document, 'script', 'twitter-wjs');
                        </script>
                    </div>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>
        <div id="page-wrapper">
            @RenderBody()
            @Html.Partial("_Adsense")
        </div>
        <div class="footer">
            <footer class="footer">
                <p>&copy; @DateTime.Now.Year - AzureSpeed.com | By <a href="http://blchen.com" target="_blank">Blair Chen</a>, using <a href="https://github.com/orcame/jazure" target="_blank">jazure</a>
            </footer>
        </div>
    </div>

    <environment names="Development">
        <script src="~/lib/bootstrap/js/bootstrap.js"></script>
        <script src="~/lib/angular/angular.js"></script>
        <script src="~/lib/angular-filter/angular-filter.js"></script>
        <script src="~/lib/angular-ui-bootstrap/ui-bootstrap.js"></script>
        <script src="~/lib/checklist-model/checklist-model.js"></script>
        <script src="~/lib/angular-local-storage/angular-local-storage.js"></script>
        <script src="~/lib/d3/d3.js"></script>
        <script src="~/lib/metisMenu/metisMenu.js"></script>
        <script src="~/js/sb-admin-2.js"></script>
        <script src="~/js/jazure/crypto-min.js"></script>
        <script src="~/js/jazure/jazure.core/jazure.core.js"></script>
        <script src="~/js/jazure/jazure.storage/jazure.storage.core.js"></script>
        <script src="~/js/jazure/jazure.storage/jazure.storage.account.js"></script>
        <script src="~/js/jazure/jazure.storage/blob/jazure.storage.container.js"></script>
        <script src="~/js/jazure/jazure.storage/blob/jazure.storage.blob.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/common.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/utils.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/controllers/MainController.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/controllers/CloudRegionFinderController.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/controllers/DownloadController.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/controllers/LatencyController.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/controllers/IpRangeController.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/controllers/UploadController.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed/controllers/UploadLargeFileController.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.4.min.js"
                asp-fallback-src="~/lib/jquery/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script src="~/js/lib.all.min.js"></script>
        <script src="~/js/jazure.all.min.js" asp-append-version="true"></script>
        <script src="~/js/azurespeed.all.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
